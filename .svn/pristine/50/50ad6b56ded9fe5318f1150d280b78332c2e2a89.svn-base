# log4j.rootLogger=error, stdout

log4j.logger.Dialog.Designer.TraceWriter.RCOM_CF_RDTV_9002= debug, TRCOM_CF_RDTV_9002
log4j.logger.Dialog.Designer.ReportWriter.RCOM_CF_RDTV_9002= info, RRCOM_CF_RDTV_9002
log4j.logger.flow.Capture_ANI_DNIS_BC = info, TRCOM_CF_RDTV_9002
log4j.logger.com.rcom.rdtv = info, TRCOM_CF_RDTV_9002
# Console
log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n

# Report File.
log4j.appender.RRCOM_CF_RDTV_9002=org.apache.log4j.RollingFileAppender
log4j.appender.RRCOM_CF_RDTV_9002.MaxFileSize=5000KB
log4j.appender.RRCOM_CF_RDTV_9002.MaxBackupIndex=10
log4j.appender.RRCOM_CF_RDTV_9002.File=D:/RCOM/OtherFlows/RDTV_9002/Logs/report.log
log4j.appender.RRCOM_CF_RDTV_9002.DatePattern='.'yyyy-MM-dd
log4j.appender.RRCOM_CF_RDTV_9002.layout=org.apache.log4j.PatternLayout
log4j.appender.RRCOM_CF_RDTV_9002.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n
log4j.appender.RRCOM_CF_RDTV_9002.Encoding=UTF-8
# Trace File.
log4j.appender.TRCOM_CF_RDTV_9002=org.apache.log4j.RollingFileAppender
log4j.appender.TRCOM_CF_RDTV_9002.MaxFileSize=5MB
log4j.appender.TRCOM_CF_RDTV_9002.MaxBackupIndex=10
log4j.appender.TRCOM_CF_RDTV_9002.File=D:/RCOM/OtherFlows/RDTV_9002/Logs/trace.log
log4j.appender.TRCOM_CF_RDTV_9002.DatePattern='.'yyyy-MM-dd
log4j.appender.TRCOM_CF_RDTV_9002.layout=org.apache.log4j.PatternLayout
log4j.appender.TRCOM_CF_RDTV_9002.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n
log4j.appender.TRCOM_CF_RDTV_9002.Encoding=UTF-8
