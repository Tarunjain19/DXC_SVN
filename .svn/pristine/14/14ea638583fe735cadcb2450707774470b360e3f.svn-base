package com.servion.rcom.hni.hostinterface.view;

import com.servion.rcom.hni.hostinterface.util.HostConstants;

public class ReqActivateVAS {

	private String MDN = null;
	private String packCode = null;
	public String getMDN() {
		return MDN;
	}
	public void setMDN(String mDN) {
		MDN = mDN;
	}
	public String getPackCode() {
		return packCode;
	}
	public void setPackCode(String packCode) {
		this.packCode = packCode;
	}
	
	public String getParamValue()
	{
		return "MDN : " +this.MDN + HostConstants.PIPE + "Pack code : " +this.packCode;
	}
}
