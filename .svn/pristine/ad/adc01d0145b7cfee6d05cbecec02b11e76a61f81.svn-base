log4j.rootLogger=error, stdout

log4j.logger.Dialog.Designer.TraceWriter.RCOM_CF_WebworldHelpdesk_IVR= debug, TRCOM_CF_WebworldHelpdesk_IVR
log4j.logger.Dialog.Designer.ReportWriter.RCOM_CF_WebworldHelpdesk_IVR= debug, RRCOM_CF_WebworldHelpdesk_IVR
log4j.logger.com.servion.util.StackTrace = debug, RRCOM_CF_WebworldHelpdesk_IVR
log4j.logger.flow.CallStart = debug, dblog
log4j.logger.com.rcom.mq.utils.LoggerUtil=debug,client

# Console
log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n

# Report File.
log4j.appender.RRCOM_CF_WebworldHelpdesk_IVR=org.apache.log4j.RollingFileAppender
log4j.appender.RRCOM_CF_WebworldHelpdesk_IVR.File=D:/RCOM/OtherFlows/RCOM_CF_WebworldHelpdesk_IVR/log/report.log
log4j.appender.RRCOM_CF_WebworldHelpdesk_IVR.MaxFileSize=30000KB
log4j.appender.RRCOM_CF_WebworldHelpdesk_IVR.MaxBackupIndex=10
log4j.appender.RRCOM_CF_WebworldHelpdesk_IVR.DatePattern='.'yyyy-MM-dd
log4j.appender.RRCOM_CF_WebworldHelpdesk_IVR.layout=org.apache.log4j.PatternLayout
log4j.appender.RRCOM_CF_WebworldHelpdesk_IVR.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n
log4j.appender.RRCOM_CF_WebworldHelpdesk_IVR.Encoding=UTF-8
# Trace File.
log4j.appender.TRCOM_CF_WebworldHelpdesk_IVR=org.apache.log4j.RollingFileAppender
log4j.appender.TRCOM_CF_WebworldHelpdesk_IVR.File=D:/RCOM/OtherFlows/RCOM_CF_WebworldHelpdesk_IVR/log/trace.log
log4j.appender.TRCOM_CF_WebworldHelpdesk_IVR.MaxFileSize=30000KB
log4j.appender.TRCOM_CF_WebworldHelpdesk_IVR.MaxBackupIndex=10
log4j.appender.TRCOM_CF_WebworldHelpdesk_IVR.DatePattern='.'yyyy-MM-dd
log4j.appender.TRCOM_CF_WebworldHelpdesk_IVR.layout=org.apache.log4j.PatternLayout
log4j.appender.TRCOM_CF_WebworldHelpdesk_IVR.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n
log4j.appender.TRCOM_CF_WebworldHelpdesk_IVR.Encoding=UTF-8

# Trace File.
log4j.appender.dblog=org.apache.log4j.DailyRollingFileAppender
log4j.appender.dblog.File=D:/RCOM/RCOM_CF_WebworldHelpdesk_IVR/log/dblog.log
log4j.appender.dblog.DatePattern='.'yyyy-MM-dd
log4j.appender.dblog.layout=org.apache.log4j.PatternLayout
log4j.appender.dblog.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n
log4j.appender.dblog.Encoding=UTF-8


log4j.appender.client=org.apache.log4j.RollingFileAppender
log4j.appender.client.MaxFileSize=10000KB
log4j.appender.client.MaxBackupIndex=50
log4j.appender.client.File=D:/RCOM/RCOM_CF_WebworldHelpdesk_IVR/log/mqlog.log
log4j.appender.client.layout=org.apache.log4j.PatternLayout
log4j.appender.client.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n
log4j.appender.client.Encoding=UTF-8
