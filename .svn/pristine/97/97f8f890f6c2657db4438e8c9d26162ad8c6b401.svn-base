package com.servion.rcom.ppp.hostinterface.view;

import com.servion.rcom.ppp.hostinterface.util.HostConstants;

public class OfferDetails {

	private String campaignName = null;
	private String campaignText = null;
	private String mrp = null;
	private String validity = null;
	
	
	public String getCampaignName() {
		return campaignName;
	}
	public void setCampaignName(String campaignName) {
		this.campaignName = campaignName;
	}
	public String getCampaignText() {
		return campaignText;
	}
	public void setCampaignText(String campaignText) {
		this.campaignText = campaignText;
	}
	public String getMrp() {
		return mrp;
	}
	public void setMrp(String mrp) {
		this.mrp = mrp;
	}
	public String getValidity() {
		return validity;
	}
	public void setValidity(String validity) {
		this.validity = validity;
	}

	@Override
	public String toString() {
		return campaignName + HostConstants.PIPE + campaignText + HostConstants.PIPE
				+ mrp + HostConstants.PIPE + validity;
	}
	
	
	
}
