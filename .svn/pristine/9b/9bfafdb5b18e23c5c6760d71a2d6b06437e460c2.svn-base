log4j.rootLogger=error, stdout

log4j.logger.Dialog.Designer.TraceWriter.RCOM_CF_DND_1909_IVR= debug, TRCOM_CF_DND_1909_IVR
log4j.logger.Dialog.Designer.ReportWriter.RCOM_CF_DND_1909_IVR= debug, RRCOM_CF_DND_1909_IVR
log4j.logger.com.dnd.util.StackTrace = debug, TRCOM_CF_DND_1909_IVR
log4j.logger.flow.CallStart = debug, dblog
log4j.logger.com.rcom.mq.utils.LoggerUtil=debug,client

# Console
log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n

# Report File.
log4j.appender.RRCOM_CF_DND_1909_IVR=org.apache.log4j.RollingFileAppender
log4j.appender.RRCOM_CF_DND_1909_IVR.File=D:/RCOM/logs/DND/report.log
log4j.appender.RRCOM_CF_DND_1909_IVR.maxFileSize=50MB
log4j.appender.RRCOM_CF_DND_1909_IVR.maxBackupIndex=5
log4j.appender.RRCOM_CF_DND_1909_IVR.DatePattern='.'yyyy-MM-dd
log4j.appender.RRCOM_CF_DND_1909_IVR.layout=org.apache.log4j.PatternLayout
log4j.appender.RRCOM_CF_DND_1909_IVR.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n
log4j.appender.RRCOM_CF_DND_1909_IVR.Encoding=UTF-8

# Trace File.
log4j.appender.TRCOM_CF_DND_1909_IVR=org.apache.log4j.RollingFileAppender
log4j.appender.TRCOM_CF_DND_1909_IVR.File=D:/RCOM/logs/DND/trace.log
log4j.appender.TRCOM_CF_DND_1909_IVR.maxFileSize=50MB
log4j.appender.TRCOM_CF_DND_1909_IVR.maxBackupIndex=50
log4j.appender.TRCOM_CF_DND_1909_IVR.DatePattern='.'yyyy-MM-dd
log4j.appender.TRCOM_CF_DND_1909_IVR.layout=org.apache.log4j.PatternLayout
log4j.appender.TRCOM_CF_DND_1909_IVR.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n
log4j.appender.TRCOM_CF_DND_1909_IVR.Encoding=UTF-8

# Trace File.
log4j.appender.dblog=org.apache.log4j.DailyRollingFileAppender
log4j.appender.dblog.File=D:/RCOM/logs/DND/dblog.log
log4j.appender.dblog.maxFileSize=50MB
log4j.appender.dblog.maxBackupIndex=5
log4j.appender.dblog.DatePattern='.'yyyy-MM-dd
log4j.appender.dblog.layout=org.apache.log4j.PatternLayout
log4j.appender.dblog.layout.ConversionPattern=%d{dd/MM/yyyy HH:mm:ss:SSS} %5p - %m%n
log4j.appender.dblog.Encoding=UTF-8
