	@Override
	public void requestBegin(SCESession mySession) {



		ITraceInfo trace = mySession.getTraceOutput();
		String key = AppConstant.EMPTY_STRING,appName = AppConstant.EMPTY_STRING;
		String selectedValue =AppConstant.EMPTY_STRING;
		String talkTime =AppConstant.EMPTY_STRING;
		ArrayList<String> dynamicValue = null;
		String initialPrompt = AppConstant.EMPTY_STRING;
		HashMap <String, String> mapDenominationDetail = null;
		HashMap <String, String> mapMRPdetail =null,mapSTVdetail =null; 
		String detailPhrase = AppConstant.EMPTY_STRING;
		String mrpValue = AppConstant.EMPTY_STRING,STVcode = AppConstant.EMPTY_STRING;
		//initial prompt processing...
		try
		{
			appName = mySession.getVariableField(IProjectVariables.APP_NAME).getStringValue();
			selectedValue = mySession.getVariableField(IProjectVariables.PACK_DETAILS_MENU,IProjectVariables.PACK_DETAILS_MENU_FIELD_VALUE).getStringValue();
			
			trace.writeln(ITraceInfo.TRACE_LEVEL_INFO, "Application Name :: "+appName);
			trace.writeln(ITraceInfo.TRACE_LEVEL_INFO, "selected Value :: "+selectedValue);
			
			key = appName +AppConstant.SEPERATOR_HYPEN+ AppConstant.MN_0023;
			trace.writeln(ITraceInfo.TRACE_LEVEL_INFO, "Key :: "+key);
			initialPrompt = AppProperties.GetInstance(Constants.AUDIO_CONFIG).getIVRProp(key);
			
			mapDenominationDetail = (HashMap <String, String>) mySession.getProperty(AppConstant.STV_DENOMINATION_DETAILS_MAP);
			detailPhrase = mapDenominationDetail.get(selectedValue);
			
			dynamicValue = new ArrayList<String>();
			
			trace.writeln(ITraceInfo.TRACE_LEVEL_INFO, "STV  Confirmation initial Prompt Before Dynamic Insertion:: "+initialPrompt);		
			dynamicValue.add(detailPhrase);
			
			trace.writeln(ITraceInfo.TRACE_LEVEL_INFO, "dynamicValue :"+dynamicValue);
			
			DynamicMenu objDynamicMenu = new DynamicMenu();
			initialPrompt=objDynamicMenu.playPrompt(dynamicValue, initialPrompt, AppConstant.NA, trace);
			
			trace.writeln(ITraceInfo.TRACE_LEVEL_INFO, "STV  Confirmation initial Prompt :: "+initialPrompt);
			mySession.getVariableField(IProjectVariables.PREPAID__DM_INPUT_PARAMETER, IProjectVariables.PREPAID__DM_INPUT_PARAMETER_FIELD_INITIAL_PROMPT).setValue(initialPrompt);
			
			/***Set Amount***/
			mapMRPdetail = (HashMap <String, String>) mySession.getProperty(AppConstant.STV_MRP_MAP);
			mrpValue = mapMRPdetail.get(selectedValue);
			trace.writeln(ITraceInfo.TRACE_LEVEL_INFO, "STV MRP Value :: "+mrpValue);
			mySession.getVariableField(IProjectVariables.PREPAID__APP_VARIABLES,IProjectVariables.PREPAID__APP_VARIABLES_FIELD_AMOUNT).setValue(mrpValue);
			
			/**Set RC Type**/
			mapSTVdetail =(HashMap <String, String>)mySession.getProperty(AppConstant.STV_OFFER_ID);
			STVcode = mapSTVdetail.get(selectedValue);
			trace.writeln(ITraceInfo.TRACE_LEVEL_INFO, "STVcode :: "+STVcode);
			mySession.getVariableField(IProjectVariables.PREPAID__APP_VARIABLES,IProjectVariables.PREPAID__APP_VARIABLES_FIELD_RCTYPE).setValue(STVcode);
			
		}
		catch(Exception e)
		{
			PrintStackTrace.PrintException(e);
			trace.writeln(ITraceInfo.TRACE_LEVEL_ERROR, "Exception in Top Up Confirmation initial prompt :: "+e);
		}
		finally
		{
				initialPrompt = null;
		}
	
	}
	
}
